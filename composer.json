{
    "name": "marc-mabe/php-enum-phpstan",
    "description": "Enum class reflection extension for PHPStan",
    "type": "phpstan-extension",
    "keywords": ["enum", "phpstan"],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 | ^8.0",
        "marc-mabe/php-enum": "^1.1 || ^2.0 || ^3.0 || ^4.0",
        "phpstan/phpstan": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 | ^8.5 | 9.4"
    },
    "autoload": {
        "psr-4": {
            "MabeEnumPHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MabeEnum\\PHPStan\\tests\\": "tests/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
